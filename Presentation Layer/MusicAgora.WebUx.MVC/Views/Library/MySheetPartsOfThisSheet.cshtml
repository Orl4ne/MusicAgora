@model IEnumerable<MusicAgora.Common.Library.TransferObjects.SheetPartTO>

@{
    ViewData["Title"] = "MySheetPartsOfThisSheet";
}

<h1>@Model.FirstOrDefault().Sheet.Name</h1>

<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Part)
            </th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.Part)
                </td>
                <td>
                    @Html.ActionLink("Download", "DownloadSheetPart", new { id = item.Id }, new { @class = "btn btn-success" })
                </td>
            </tr>
        }
    </tbody>
</table>
<div>
    <a asp-action="AllCurentSheets">Back to Sheets List</a>
</div>
